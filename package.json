{
	"version": "0.0.1",
	"license": "MIT",
	"name": "material-solid-table",
	"description": "A fully featured Material UI V5 implementation of TanStack Solid Table V8, written from the ground up in TypeScript.",
	"author": "Kevin Vandy",
	"keywords": [
		"data table",
		"material-ui",
		"suid",
		"solid",
		"solid-table",
		"tanstack table"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/KevinVandy/material-solid-table"
	},
	"homepage": "https://www.material-solid-table.com/",
	"bugs": {
		"url": "https://github.com/KevinVandy/material-solid-table/issues"
	},
	"type": "module",
	"size-limit": [
		{
			"path": "dist/cjs/index.js",
			"limit": "50 KB"
		},
		{
			"path": "dist/esm/material-solid-table.esm.js",
			"limit": "50 KB"
		}
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"analyze": "size-limit --why",
		"build": "rollup -c && size-limit",
		"build-storybook": "build-storybook",
		"format": "prettier -w .",
		"lint": "eslint .",
		"size": "size-limit",
		"start": "start-storybook -p 6007",
		"storybook": "start-storybook -p 6007"
	},
	"devDependencies": {
		"@babel/core": "7.19.3",
		"@faker-js/faker": "^7.6.0",
		"@storybook/addon-a11y": "^6.5.12",
		"@storybook/addon-actions": "6.5.12",
		"@storybook/addon-essentials": "6.5.12",
		"@storybook/addon-info": "^5.3.21",
		"@storybook/addon-links": "6.5.12",
		"@storybook/addon-storysource": "^6.5.12",
		"@storybook/addons": "^6.5.12",
		"@storybook/html": "6.5.12",
		"@suid/icons-material": "^0.4.1",
		"@suid/material": "^0.6.3",
		"@suid/vite-plugin": "^0.0.3",
		"babel-loader": "8.2.5",
		"prettier": "^2.7.1",
		"size-limit": "^8.1.0",
		"solid-js": "1.5.7",
		"storybook-builder-vite": "0.1.23",
		"storybook-dark-mode": "^1.1.2",
		"typescript": "4.8.4",
		"vite": "^3.1.8",
		"vite-plugin-solid": "^2.3.9"
	},
	"peerDependencies": {
		"@suid/icons-material": ">=0.4",
		"@suid/material": ">=0.6",
		"solid-js": ">=1.5"
	},
	"dependencies": {
		"@tanstack/match-sorter-utils": "8.1.1",
		"@tanstack/solid-table": "8.5.13"
	}
}
